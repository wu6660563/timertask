<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace和定义的Mapper接口对应，并实现其中的方法 -->
<mapper namespace="com.latin.api.app.mapper.VersionMapper">

	<select id="getLastObject" resultType="com.latin.api.app.model.Version">
     	SELECT * FROM oc_version ORDER BY version_id LIMIT 1
  	</select>
</mapper>

